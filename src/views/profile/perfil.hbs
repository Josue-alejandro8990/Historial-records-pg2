<div class="buscar">
  <form action="/profile/search" method="POST">
    <input type="text" name="id" placeholder="Ingresa una busqueda" class="buscar-input" autofocus>
    <button class="btn-search ">
      <i class="fas fa-search icon"></i>
    </button>
  </form>
</div>
<div class="body_paciente">
  <div class="container_perfil p-4" id="capture6">
    <div class="container-foto">
      {{#if paciente}}
      {{#each paciente}}
      <div class="form-group2">
          <a class="btn-editperfil" href="/info/edit_perfilfoto/{{id_paciente}}">
            <i class="fas fa-pencil icon"></i></a>
      </div>
      <div class="form-group">
        <img src="{{perfil}}" alt="" class="perfil">
      </div>
      {{/each}}
      {{else}}
      <img src="/img/profiledefault.png" alt="" class="perfil">
      {{/if}}
    </div>
    <div class="container-datos">
      {{#if paciente}}
      <div class="card-body">
        <div class="row">
          {{#each paciente}}
          <div class="form-group2">
              <a class="btn-editperfil2" href="/info/edit_perfilpaciente/{{id_paciente}}">
                <i class="fas fa-pencil icon"></i></a>
              <a class="btn-pdf2" onclick="makepdf6({{id_paciente}})">
                <i class="far fa-file-pdf"></i></a>
          </div>
          <div class="form-group">
            <p class="p-nombres">Nombres :</p>
            <p class="p-nombres2">{{nombres}} </p>
          </div>
          <div class="form-group">
            <p class="p-nombres">Apellidos : </p>
            <p class="p-nombres2">{{apellidos}}</p>
          </div>
          <div class="form-group">
            <p class="p-nombres">Fecha de nacimiento : </p>
            <p class="p-nombres2">{{formatDate fecha_nacimiento}} </p>
          </div>
          <div class="form-group">
            <p class="p-nombres">Edad : </p>
            <p class="p-nombres2">{{edad}} </p>
          </div>
          <div class="form-group">
            <p class="p-nombres">DPI : </p>
            <p class="p-nombres2">{{dpi}} </p>
          </div>
          <div class="form-group">
            <p class="p-nombres">Telefono : </p>
            <p class="p-nombres2">{{telefono}} </p>
          </div>
          <div class="form-group">
            <p class="p-nombres">Dirección : </p>
            <p class="p-nombres2">{{direccion}} </p>
          </div>
          <div class="form-group">
            <p class="p-nombres">Estado Civil : </p>
            <p class="p-nombres2">{{estado_civil}} </p>
          </div>
          <div class="form-group">
            <p class="p-nombres">Genero : </p>
            <p class="p-nombres2">{{genero}} </p>
          </div>
        </div>
        {{/each}}
      </div>
      {{else}}
      <h3>Aun no se ha realizado una busqueda de historial medico!!!</h3>
      {{/if}}
    </div>
  </div>

  <div class="container_historial p-4">
    <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
      <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="headingOne">
          <h4 class="panel-title">
            <a class="a-estilo" role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseOne"
              aria-expanded="true" aria-controls="collapseOne">
              Antecedentes Familiares
            </a>
          </h4>
        </div>
        <div id="collapseOne" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
          <div class="panel-body">
            <div class="container-laboratorios" name="antecedentes" id="capture">
              {{#if antecedentes}}
              <div class="encabezado">
                <h3>Antecedentes Familiares</h3>
              </div>
              <div class="laboratorio">
                {{#each antecedentes}}
                <div class="separador">
                </div>
                <div class="form-group">
                  <h3 class="controles">Fecha : {{formatDate fecha}}</h3>
                    <a class="btn-edit" href="/info/edit_antecedentes/{{id_paciente}}">
                      <i class="fas fa-pencil icon"></i></a>
                    <a class="btn-delete" href="/removeinfo/removeantecedente/{{id_antecedente}}/{{id_paciente}}">
                      <i class="	far fa-trash-alt"></i></a>
                    <a class="btn-new" href="/newinfo/newantecedente/{{id_paciente}}">
                      <i class="fas fa-plus icon" style="color-white"></i></a>
                    <a class="btn-pdf2" onclick="makepdf({{id_paciente}})">
                      <i class="far fa-file-pdf"></i></a>
                </div>
                <div class="card-body">
                  <div class="row">
                    <div class="form-group">
                      <p class="p-nombres2">CANCER:</p>
                      <p class="p-nombres2">{{cancer}}</p>
                    </div>
                    <div class="form-group">
                      <p class="p-nombres2">VIH:</p>
                      <p class="p-nombres2">{{vih}}</p>
                    </div>
                    <div class="form-group">
                      <p class="p-nombres2">ASMA: </p>
                      <p class="p-nombres2">{{asma}}</p>
                    </div>
                    <div class="form-group">
                      <p class="p-nombres2">CEGUEREA: </p>
                      <p class="p-nombres2">{{ceguera}}</p>
                    </div>
                    <div class="form-group">
                      <p class="p-nombres2">ANSIEDAD: </p>
                      <p class="p-nombres2">{{ansiedad}}</p>
                    </div>
                    <div class="form-group">
                      <p class="p-nombres2">DEPRESEION: </p>
                      <p class="p-nombres2">{{depresion}}</p>
                    </div>
                  </div>
                </div>
                {{/each}}
              </div>
              {{else}}
              <div class="titulo">
                <h3>No se le ha practicado ningun laboratorio al paciente!!</h3>
              </div>
              <div class="form-group">
                {{#each paciente}}
                  <a class="btn-edit" href="/info/edit_antecedentes/{{id_paciente}}">
                    <i class="fas fa-pencil icon"></i></a>
                  <a class="btn-delete" href="/removeinfo/removeantecedente/{{id_antecedente}}/{{id_paciente}}">
                    <i class="	far fa-trash-alt"></i></a>
                  <a class="btn-new" href="/newinfo/newantecedente/{{id_paciente}}">
                    <i class="fas fa-plus icon" style="color-white"></i></a>
                  <a class="btn-pdf2" onclick="makepdf({{id_paciente}})">
                    <i class="far fa-file-pdf"></i></a>
                {{/each}}
              </div>
              {{/if}}
            </div>
          </div>
        </div>
      </div>
      <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="headingTwo">
          <h4 class="panel-title">
            <a class="a-estilo" role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseTwo"
