                      <p class="p-nombres2">{{r_g_blancos}} mm3 </p>
                      <p class="p-nombres2"> 4,000-10,000/mm3</p>
                    </div>
                    <div class="form-group">
                      <p class="p-nombres2">Neutrofilos : </p>
                      <p class="p-nombres2">{{neutrofilos}} % </p>
                      <p class="p-nombres2">40-60%</p>
                    </div>
                    <div class="form-group">
                      <p class="p-nombres2">Eosinofilos : </p>
                      <p class="p-nombres2">{{eosinofilos}} % </p>
                      <p class="p-nombres2">1-4%</p>
                    </div>
                    <div class="form-group">
                      <p class="p-nombres2">Basofilos : </p>
                      <p class="p-nombres2">{{basofilos}} % </p>
                      <p class="p-nombres2">0-2%</p>
                    </div>
