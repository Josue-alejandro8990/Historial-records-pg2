                      <i class="	far fa-trash-alt"></i></a>
                    <a class="btn-new" href="/newinfo/newtratamiento/{{id_paciente}}">
                      <i class="fas fa-plus icon" style="color-white"></i></a>
                    <a class="btn-pdf2" onclick="makepdf5({{id_paciente}})">
                      <i class="far fa-file-pdf"></i></a>
                </div>
                <div class="card-body">
                  <div class="row">
                    <div class="col-md-4">
                      <div class="form-group">
                        <P class="p-nombres2">DESCRIPCION :</P>
                      </div>
                      <div class="form-group">
                        <p class="p-nombres2">MEDICAMENTO :</p>
                      </div>
                      <div class="form-group">
                        <p class="p-nombres2"> DOSIS :</p>
                      </div>
                      <div class="form-group">
                        <p class="p-nombres2">DURACION DEL TRATAMIENTO :</p>
                      </div>
                    </div>
                    <div class="col-md-6">
                      <div class="form-group">
                        <p class="p-datosmedicos">{{descripcion}}</p>
                      </div>
                      <div class="form-group">
                        <p class="p-datosmedicos">{{medicamento}}</p>
                      </div>
                      <div class="form-group">
                        <p class="p-datosmedicos"> {{dosis}} </p>
                      </div>
                      <div class="form-group">
                        <p class="p-datosmedicos"> {{tiempo}}</p>
                      </div>
                    </div>
                  </div>
                </div>
                {{/each}}
              </div>
              {{else}}
              <div class="titulo">
                <h3>Este paciente no tiene ningun tratamiento asignado!!</h3>
              </div>
              <div class="form-group">
                {{#each paciente}}
                <a class="btn-edit" href="/info/edit_tratamientos/{{id_paciente}}">
                  <i class="fas fa-pencil icon"></i></a>
                <a class="btn-delete" href="/removeinfo/removetratamiento/{{id_tratamiento}}/{{id_paciente}}">
                  <i class="	far fa-trash-alt"></i></a>
                <a class="btn-new" href="/newinfo/newtratamiento/{{id_paciente}}">
                  <i class="fas fa-plus icon" style="color-white"></i></a>
                <a class="btn-pdf2" onclick="makepdf5({{id_paciente}})">
                  <i class="far fa-file-pdf"></i></a>
                {{/each}}
              </div>
              {{/if}}
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
