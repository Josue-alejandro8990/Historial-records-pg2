                    <div class="form-group">
                      <p class="p-nombres2">Cayados : </p>
                      <p class="p-nombres2">{{cayados}} % </p>
                      <p class="p-nombres2">1-3%</p>
                    </div>
                    <div class="form-group">
                      <p class="p-nombres2">SERIE ROJA---</p>
                      <p class="p-nombres2">---------</p>
                      <p class="p-nombres2">-------------</p>
                    </div>
                    <div class="form-group">
                      <p class="p-nombres2">Recuento de Globulos Rojos : </p>
                      <p class="p-nombres2">{{r_g_rojos}} millones/mm3 </p>
                      <p class="p-nombres2"> 3.5-5.5millones/mm3</p>
                    </div>
                    <div class="form-group">
                      <p class="p-nombres2">Hemoglobina : </p>
                      <p class="p-nombres2">{{hemoglobina}} g/dL </p>
                      <p class="p-nombres2">11-15g/dL<br>
                        36-48%</p>
                    </div>
                    <div class="form-group">
                      <p class="p-nombres2">Hematocrito : </p>
                      <p class="p-nombres2">{{hematocrito}} % </p>
                      <p class="p-nombres2">80-100fL</p>
                    </div>
                    <div class="form-group">
                      <p class="p-nombres2">Volumen Corpuscular Media : </p>
                      <p class="p-nombres2">{{v_corupuscular_m}} fL </p>
                      <p class="p-nombres2"> 26-34pg</p>
                    </div>
                    <div class="form-group">
                      <p class="p-nombres2">Hemoglobina Corpuscular Media : </p>
                      <p class="p-nombres2">{{hemoglobina_c_m}} pg </p>
                      <p class="p-nombres2">32-36g/dL</p>
                    </div>
                    <div class="form-group">
                      <p class="p-nombres2">Concentración de Hemoglovina Corpuscular Media : </p>
                      <p class="p-nombres2">{{concentracion_hcm}} g/dL </p>
                    </div>
                    <div class="form-group">
                      <p class="p-nombres2">SERIE PLAQUETARIA---</p>
                      <p class="p-nombres2">--------</p>
                      <p class="p-nombres2">-------------</p>
                    </div>
                    <div class="form-group">
                      <p class="p-nombres2">Recuento de Plaquetas : </p>
                      <p class="p-nombres2">{{r_plaquetas}} /mm3 </p>
                      <p class="p-nombres2">100,000-300,000/mm3</p>
                    </div>
                    <div class="form-group">
                      <p class="p-nombres2">ERITROSEDIMENTACION</p>
                      <p class="p-nombres2">-----------------</p>
                      <p class="p-nombres2">-------------</p>
                    </div>
                    <div class="form-group">
                      <p class="p-nombres2">Velocidad de Eritrosedimentación :</p>
                      <p class="p-nombres2">{{velocidad_eritrodimentacion}} mm/Hora </p>
                      <p class="p-nombres2">Hombres: 0-6mm/Hora <br>Mujeres: 0-15mm/Hora</p>
                    </div>
                  </div>
                </div>
                {{/each}}
              </div>
              {{else}}
              <div class="titulo">
                <h3>No se le ha practicado ningun laboratorio al paciente!!</h3>
              </div>
              <div class="form-group">
                {{#each paciente}}
                  <a class="btn-edit" href="/info/edit_hemograma/{{id_paciente}}">
                    <i class="fas fa-pencil icon"></i></a>
                  <a class="btn-delete" href="/removeinfo/removehemograma/{{id_hemograma}}/{{id_paciente}}">
                    <i class="	far fa-trash-alt"></i></a>
                  <a class="btn-new" href="/newinfo/newhemograma/{{id_paciente}}">
                    <i class="fas fa-plus icon" style="color-white"></i></a>
                  <a class="btn-pdf2" onclick="makepdf2({{id_paciente}})">
                    <i class="far fa-file-pdf"></i></a>
                {{/each}}
              </div>
              {{/if}}
            </div>
            <div class="container-laboratorios" name="perfil_renal" id="capture3">
